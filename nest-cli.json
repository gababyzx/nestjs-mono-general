{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/baam/src",
  "monorepo": true,
  "root": "apps/baam",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/baam/tsconfig.app.json",
    "assets": [
      "env/**"
    ]
  },
  "projects": {
    "baam": {
      "type": "application",
      "root": "apps/baam",
      "entryFile": "main",
      "sourceRoot": "apps/baam/src",
      "compilerOptions": {
        "webpack": false,
        "tsConfigPath": "apps/baam/tsconfig.app.json"
      }
    },
    "share": {
      "type": "library",
      "root": "libs/share",
      "entryFile": "index",
      "sourceRoot": "libs/share/src",
      "compilerOptions": {
        "webpack": false,
        "tsConfigPath": "libs/share/tsconfig.lib.json"
      }
    }
  }
}